# Development Environment Variables
# These are for local development only

# Application Info
NEXT_PUBLIC_APP_NAME="Suara (Dev)"
NEXT_PUBLIC_APP_VERSION="1.0.0-dev"
NEXT_PUBLIC_APP_ENV="development"

# Azure Configuration (for testing)
NEXT_PUBLIC_AZURE_STORAGE_ACCOUNT="productionsuara"
NEXT_PUBLIC_AZURE_REGION="southeastasia"

# API Endpoints (localhost for development)
# NEXT_PUBLIC_API_BASE_URL="http://localhost:3001"

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_MONITORING=false

# Performance
NEXT_PUBLIC_ENABLE_SERVICE_WORKER=false

# Development
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1
